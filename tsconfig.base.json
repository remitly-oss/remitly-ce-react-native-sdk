{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "es2015",
            "es2016",
            "es2017.object"
        ],
        "experimentalDecorators": true,
        "downlevelIteration": true,
        "allowUnreachableCode": false,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
    }
}
